<?xml version="1.0" encoding="utf-8"?>
<s:Application xmlns:fx="http://ns.adobe.com/mxml/2009" 
			   xmlns:s="library://ns.adobe.com/flex/spark" 
			   xmlns:mx="library://ns.adobe.com/flex/mx" 
			   xmlns:core="net.fproject.gui.component.*"
			   xmlns:gui="gui.*" currentState="bantin"
			   xmlns:even="event.*">
	
	<fx:Style source="style.css" />
	
	<fx:Script>
		<![CDATA[
			import event.StatesEvents;
			/*
			public static function getTopAplication():DisplayObject{
				return this;
			}*/
			protected function menu3_clickBtHandler(e:StatesEvents):void
			{
				// TODO Auto-generated method stub
				if(e.idBt == "btbantin"){
					currentState = "bantin";
				} else if( e.idBt == "btnhanvien"){
					currentState = "nhanvien"
				} else if( e.idBt == "btycnghiphep"){
					currentState = "ycNghiphep"
				} else if( e.idBt == "btlsnghi"){
					currentState = "lsNghiphep";
				} else if( e.idBt == "btpheduyetyc"){
					currentState = "pheduyet";
				} else if( e.idBt == "btTaikhoan"){
					currentState = "qlTaikhoan";
				}
			}
		]]>
	</fx:Script>
	
	<s:states>
		<s:State name="bantin" />
		<s:State name="nhanvien" />
		<s:State name="ycNghiphep" />
		<s:State name="lsNghiphep" />
		<s:State name="pheduyet" />
		<s:State name="qlTaikhoan"/>
	</s:states>
	
	
	<s:Group width="100%" height="40">
		<core:SolidColorRect width="100%" height="100%" styleName="maunen" />
		<gui:Header width="100%"></gui:Header>
	</s:Group>
	<s:HGroup  top="40" width="100%" gap="0">
		<s:Group width="14%" height="620">
			<gui:Menu width="100%" clickBt="menu3_clickBtHandler(event)"></gui:Menu>
		</s:Group>
		
		<s:Group width="86%">
			<gui:BanTin width="100%" height="620" includeIn="bantin">
				
			</gui:BanTin>
			<!--<gui:NhanVien  height="620" includeIn="nhanvien">				
			</gui:NhanVien>-->
			<!--<gui:NhanVienNew  height="620" includeIn="nhanvien">
				
			</gui:NhanVienNew>-->
			<gui:QLNhanvien width="100%" height="620" includeIn="nhanvien">
				
			</gui:QLNhanvien>
			<gui:QLTaiKhoan width="100%" height="620" includeIn="qlTaikhoan">
				
			</gui:QLTaiKhoan>
			<gui:YCNghiPhep width="100%" height="620" includeIn="ycNghiphep">
				
			</gui:YCNghiPhep>
			<gui:LichSuNghi width="100%" height="620" includeIn="lsNghiphep">
				
			</gui:LichSuNghi>
			<gui:PheDuyet width="100%" height="620" includeIn="pheduyet">
				
			</gui:PheDuyet>
		</s:Group>
	</s:HGroup>
</s:Application>
